package Library;

import java.util.*;

class Book{
	String name, author ;
	int isbn ;
	
	Book(String name , String author , int isbn){
		this.name = name ;
		this.author = author;
		this.isbn = isbn;
	}
	
	@Override
	public String toString() {
		return "Book Name: " + BookName + ", Author: " + AuthorName + ", ISBN: " + ISBNNo;
	}
}

